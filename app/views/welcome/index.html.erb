<h1>This better look like bootstrap!</h1>

<div>
	<% if current_user %>
		<% if current_user.is_admin? %>
			<%= link_to "Admin", admin_root_path %>
		<% end %>
		<%= link_to "Log Out", auth_path, method: :delete %>
	<% else %>
		<%= link_to "Sign Up", new_user_path %> | <%= link_to "Login", new_auth_path %>
	<% end %>
</div>